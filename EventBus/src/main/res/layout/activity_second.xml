<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ordinaryEvent.SecondActivity">

    <TextView
        android:id="@+id/vTextView"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/vButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="发送 Hello EventBus"
        app:layout_constraintTop_toBottomOf="@+id/vTextView" />

    <Button
        android:id="@+id/vButton2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="发送 床前明月光"
        app:layout_constraintTop_toBottomOf="@+id/vButton" />

    <Button
        android:id="@+id/vButton3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="发送 疑是地下霜"
        app:layout_constraintTop_toBottomOf="@+id/vButton2" />

    <Button
        android:id="@+id/vButton4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="发送 举头望明月"
        app:layout_constraintTop_toBottomOf="@+id/vButton3" />

    <Button
        android:id="@+id/vButton5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="发送 低头思故乡"
        app:layout_constraintTop_toBottomOf="@+id/vButton4" />

</androidx.constraintlayout.widget.ConstraintLayout>